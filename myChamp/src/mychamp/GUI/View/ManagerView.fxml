<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" maxWidth="400.0" prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: Black;" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mychamp.GUI.Controller.ManagerViewController">
   <children>
      <ImageView fitHeight="80.0" fitWidth="175.0" layoutX="286.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../Assets/football-manager-14-logo1.png" />
         </image>
      </ImageView>
      <ComboBox fx:id="cbRound" layoutX="30.0" layoutY="68.0" prefWidth="150.0" promptText="Select round" AnchorPane.leftAnchor="30.0" />
      <ComboBox fx:id="cbGroup" layoutX="30.0" layoutY="138.0" prefWidth="150.0" promptText="Select group" AnchorPane.leftAnchor="30.0" />
      <ComboBox fx:id="cbTeam" layoutX="30.0" layoutY="214.0" prefWidth="150.0" AnchorPane.leftAnchor="30.0" />
      <Label layoutX="29.0" layoutY="40.0" text="Round" textFill="WHITE" AnchorPane.leftAnchor="30.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="31.0" layoutY="111.0" text="Group" textFill="WHITE" AnchorPane.leftAnchor="30.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="34.0" layoutY="187.0" text="Team" textFill="WHITE" AnchorPane.leftAnchor="30.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnHandleResult" layoutX="311.0" layoutY="263.0" mnemonicParsing="false" onAction="#handleGoToResultManager" text="Go" />
      <Separator layoutY="305.0" prefHeight="0.0" prefWidth="400.0" />
      <Label layoutX="30.0" layoutY="323.0" text="Remove team from tournament" textFill="WHITE" AnchorPane.leftAnchor="30.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnRemoveWndw" layoutX="312.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleRemoveWndw" text="Go" />
      <Button fx:id="btnBack" layoutX="29.0" layoutY="263.0" mnemonicParsing="false" onAction="#handleBack" text="Back" AnchorPane.leftAnchor="30.0" />
   </children>
</AnchorPane>
